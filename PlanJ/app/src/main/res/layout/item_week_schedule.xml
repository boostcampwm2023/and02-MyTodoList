<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="1.5dp">

    <View
        android:id="@+id/vw_schedule_color_bar"
        android:layout_width="2dp"
        android:layout_height="14dp"
        android:layout_marginStart="1dp"
        android:background="@color/main3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/vw_schedule_Title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="가가가각가가가"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@id/vw_schedule_color_bar"
        app:layout_constraintTop_toTopOf="@id/vw_schedule_color_bar" />


</androidx.constraintlayout.widget.ConstraintLayout>